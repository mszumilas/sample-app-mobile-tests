{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "types": ["node", "@wdio/globals"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "./dist"
  },
  "include": ["./tests/**/*.ts", "./pageobjects/**/*.ts"],
  "exclude": ["dist"]
}
